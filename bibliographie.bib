issn = {2041-2649},
journal = {Briefings in Functional Genomics},
keywords = {GeneCoExpression,GeneNetwork,co-expression networks,functional pathways,master genetic regulators,transcriptional regulation},
mendeley-groups = {Ph.D CHUL},
mendeley-tags = {GeneCoExpression,GeneNetwork},
number = {1},
pages = {66--78},
pmid = {23960099},
title = {{Leveraging gene co-expression networks to pinpoint the regulation of complex traits and disease, with a focus on cardiovascular traits}},
url = {https://academic.oup.com/bfg/article-lookup/doi/10.1093/bfgp/elt030},
volume = {13},
