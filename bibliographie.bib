isbn = {0000000000000},
issn = {15710645},
journal = {Physics of Life Reviews},
keywords = {bold,fmri,methods,resting-state},
month = {feb},
number = {3},
pages = {289--313},
pmid = {24655651},
title = {{The emergence of modularity in biological systems}},
